{
  "gateway": {
    "mode": "local",
    "bind": "0.0.0.0",
    "port": 8000,
    "auth": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "zai/glm-4.7",
        "fallback": "openai/gpt-4o"
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "token": "${TELEGRAM_BOT_TOKEN}"
    }
  },
  "logging": {
    "level": "info"
  }
}